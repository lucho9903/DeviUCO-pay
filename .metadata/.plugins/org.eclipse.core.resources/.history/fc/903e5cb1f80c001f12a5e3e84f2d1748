package co.edu.uco.deviucopay.crosscutting.exceptions.customs;
import co.edu.uco.deviucopay.crosscutting.exceptions.DeviUcopayxception;

public class BusinessDeviUcopayException {
	public final class BusinessDeviUcopayException extends DeviUcopayxception{

		private static final long serialVersionUID = -3662331984905572117L;

		public BusinessDeviUcopayxception(final String mensajeUsuario , Lugar lugar) {
			super(mensajeUsuario, Lugar.BUSINESS);
		}
		
		public BusinessDeviUcopayxception(final String mensajeTecnico,final String mensajeUsuario, 
				final Throwable exceptionRaiz) {
			super(mensajeTecnico,mensajeUsuario, Lugar.BUSINESS, exceptionRaiz);
		}
		
		
		
	}

